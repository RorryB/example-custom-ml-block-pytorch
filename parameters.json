{
    "version": 1,
    "type": "machine-learning",
    "info": {
        "name": "PyTorch MLP w/ CUDA",
        "description": "Test w/ cuda drivers",
        "operatesOn": "other",
        "indRequiresGpu": false,
        "repositoryUrl": "https://github.com/edgeimpulse/example-custom-ml-block-pytorch"
    },
    "parameters": [
        {
            "name": "Number of training cycles",
            "value": "100",
            "type": "int",
            "help": "Number of epochs to train the neural network on.",
            "param": "epochs"
        },
        {
            "name": "Batch Size",
            "value": "32",
            "type": "int",
            "param": "batch-size"
        },
        {
            "name": "Learning rate",
            "value": "0.001",
            "type": "float",
            "help": "How fast the neural network learns, if the network overfits quickly, then lower the learning rate.",
            "param": "learning-rate"
        },
        {
            "name": "Seed",
            "value": "0",
            "type": "int",
            "help": "Controls the random seed.",
            "param": "seed"
        },
        {
            "name": "Dropout",
            "value": "0.1",
            "type": "float",
            "param": "dropout"
        },
        {
            "name": "Number of Convolutions",
            "value": "3",
            "type": "select",
            "valid": [
                "1",
                "2",
                "3"
            ],
            "configType": "int",
            "param": "num-conv"
        },
        {
            "name": "Number of Linear Layers",
            "value": "2",
            "type": "select",
            "valid": [
                "1",
                "2"
            ],
            "configType": "int",
            "param": "num-linear"
        },
        {
            "name": "Network Width",
            "value": "2.0",
            "type": "float",
            "param": "network-width",
            "help": "Width multiplier for channels."
        },
        {
            "name": "Noise Standard Deviation",
            "value": "0.0",
            "type": "float",
            "param": "noise-std",
            "help": "Gaussian noise stddev during training."
        },
        {
            "name": "Channel Growth Mode",
            "value": "5",
            "type": "select",
            "valid": [
                "0",
                "1",
                "2",
                "3",
                "4",
                "5"
            ],
            "configType": "int",
            "param": "channel-growth-mode"
        },
        {
            "name": "Dendritic Optimization",
            "value": "false",
            "type": "boolean",
            "help": "Controls whether dendritic optimization should be applied.",
            "param": "dendritic-optimization"
        },
        {
            "name": "Speed Switch",
            "value": "slow",
            "type": "select",
            "help": "Controls the speed to switch.",
            "valid": [
                "slow",
                "medium",
                "fast"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "switch-speed"
        },
        {
            "name": "Maximum number of Dendrites",
            "value": "3",
            "type": "int",
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "max-dendrites"
        },
        {
            "name": "Improvement Threshold",
            "value": "medium",
            "type": "select",
            "valid": [
                "low",
                "medium",
                "high"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "improved-threshold"
        },
        {
            "name": "Dendrite Weight Initialization Multiplier",
            "value": "0.01",
            "type": "float",
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "dendrite-weight-initialization-multiplier"
        },
        {
            "name": "Dendrite Forward Function",
            "value": "tanh",
            "type": "select",
            "valid": [
                "relu",
                "sigmoid",
                "tanh"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "dendrite-forward-function"
        },
        {
            "name": "Dendrite Conversion",
            "value": "All Layers",
            "type": "select",
            "valid": [
                "Linear Only",
                "All Layers"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "dendrite-conversion"
        },
        {
            "name": "Improved Dendritic Optimization",
            "value": "false",
            "type": "boolean",
            "help": "Controls whether improved dendritic optimization should be applied.",
            "param": "improved-dendritic-optimization"
        },
        {
            "name": "Perforated AI Token",
            "value": "",
            "type": "string",
            "help": "Your PerforatedAI API token.",
            "showIf": {
                "parameter": "improved-dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "perforated-ai-token"
        }
    ]
}